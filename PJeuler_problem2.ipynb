   "source": [
    "sum = 0#該当する数を足し合わせる箱\n",
    "max = 4000000\n",
    "a = 1\n",
    "b = 2\n",
    "c = a + b\n",
    "\n",
    "while c <= max:#数値が偶数ならsumにその数を追加・次の項へ、それ以外であれば次の項へと進む\n",
    "    if c % 2 == 0:\n",
    "        sum += c\n",
    "        a = b\n",
    "        b = c\n",
    "        c = a + b\n",
    "    else:\n",
    "        sum += 0\n",
    "        a = b\n",
    "        b = c\n",
    "        c = a + b\n",
    "print(sum)"
   ]

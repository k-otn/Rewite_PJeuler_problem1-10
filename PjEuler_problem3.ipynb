   "source": [
    "list = []\n",
    "num = 600851475143 \n",
    "i = 2#一番最初の素因数\n",
    "while num > 1:\n",
    "    if num % i == 0:\n",
    "        list.append(i)#因数をリストに追加していく\n",
    "        num = num / i#次の因数で割る数が変わる\n",
    "    else:\n",
    "        i += 1#因数でなければ割った数に1を加え回す\n",
    "print(list)#因数のリスト"
   ]
  },
  {
